<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Percentage">
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="backLineColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="PerElectric">
        <attr name="perineHeight" format="dimension"/>
        <attr name="radius" format="dimension"/>
        <attr name="frameWidth" format="dimension"/>
        <attr name="padding" format="dimension"/>
        <attr name="margin" format="dimension"/>
        <attr name="normalColor" format="color"/>
        <attr name="lowColor" format="color"/>
    </declare-styleable>
</resources>